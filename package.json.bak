{
  "name": "tetservice-frontend",
  "version": "0.1.0",
  "scripts": {
    "start": "node server.js",
    "dev:start": "ng serve",
    "dev:build": "ng build --prod",
    "dev:test": "ng test",
    "dev:lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --quiet --fix",
    "dev:format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "dev:e2e": "ng e2e",
    "postinstall": "ngcc",
    "prepare": "husky install"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "node ng-lint-staged.js dev:lint",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@angular/animations": "11.0.3",
    "@angular/common": "11.0.3",
    "@angular/compiler": "11.0.3",
    "@angular/core": "11.0.3",
    "@angular/forms": "11.0.3",
    "@angular/localize": "11.0.3",
    "@angular/platform-browser": "11.0.3",
    "@angular/platform-browser-dynamic": "11.0.3",
    "@angular/router": "11.0.3",
    "@ng-bootstrap/ng-bootstrap": "8.0.0",
    "bootstrap": "4.5.3",
    "core-js": "3.8.0",
    "express": "4.17.1",
    "ng-http-loader": "^9.1.0",
    "ngx-toastr": "13.2.0",
    "rxjs": "6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.3",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "11.0.3",
    "@angular/compiler-cli": "11.0.3",
    "@angular/language-service": "11.0.3",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "admin-lte": "3.1.0-rc",
    "codelyzer": "^6.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "latest",
    "husky": "^7.0.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "10.5.3",
    "prettier": "^2.4.1",
    "pretty-quick": "3.1.0",
    "protractor": "7.0.0",
    "ts-node": "9.1.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.0.5"
  }
}
